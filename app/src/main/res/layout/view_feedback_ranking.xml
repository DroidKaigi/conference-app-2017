<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

        <LinearLayout
                android:id="@+id/ranking_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                />

        <TextView
                android:id="@+id/txt_label_start"
                style="@style/FeedbackRankingLabel"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@id/ranking_container"
                android:layout_marginLeft="@dimen/space_8dp"
                android:layout_marginStart="@dimen/space_8dp"
                android:layout_marginTop="@dimen/space_8dp"
                android:visibility="gone"
                />

        <TextView
                android:id="@+id/txt_label_end"
                style="@style/FeedbackRankingLabel"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@id/ranking_container"
                android:layout_marginEnd="@dimen/space_8dp"
                android:layout_marginRight="@dimen/space_8dp"
                android:layout_marginTop="@dimen/space_8dp"
                android:visibility="gone"
                />

    </RelativeLayout>

</layout>
