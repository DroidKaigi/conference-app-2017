<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SettingSwitchRow">
        <attr name="settingTitle" format="string" />
        <attr name="settingDescription" format="string" />
    </declare-styleable>

    <declare-styleable name="InfoRow">
        <attr name="infoTitle" format="string" />
        <attr name="infoDescription" format="string" />
    </declare-styleable>

    <declare-styleable name="FeedbackRankingView">
        <attr name="rankingMaxSize" format="integer" />
        <attr name="rankingLabelStart" format="string" />
        <attr name="rankingLabelEnd" format="string" />
    </declare-styleable>

    <declare-styleable name="QuestionnaireView">
        <attr name="questionnaireTitle" format="string" />
        <attr name="questionnaireDescription" format="string" />
        <attr name="hasItemOther" format="boolean" />
    </declare-styleable>

    <declare-styleable name="QuestionnaireRadioView">
        <attr name="radioItemsLabel" format="reference" />
        <attr name="radioItemsPostValueArray" format="reference" />
    </declare-styleable>

    <declare-styleable name="QuestionnaireCheckBoxView">
        <attr name="checkItemsLabel" format="reference" />
        <attr name="checkItemsPostValueArray" format="reference" />
    </declare-styleable>

    <declare-styleable name="QuestionnaireRankingView">
        <attr name="rankingItemsPostValueArray" format="reference" />
    </declare-styleable>
</resources>
